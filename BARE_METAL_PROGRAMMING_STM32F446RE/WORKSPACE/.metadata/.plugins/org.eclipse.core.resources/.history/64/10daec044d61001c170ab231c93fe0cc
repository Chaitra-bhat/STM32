//driver specific header files
#include<adc.h>
#include<uart.h>
#include<systick.h>

// SOme additonal header files
#include<stdint.h>
#include<stdio.h>
#include<stm32f4xx.h>

int sensor_value;

int main(void)
{

	RCC->AHB1ENR |= (1U<<0);

	GPIOA->MODER |= (1U<<10);

	usart2_txRx_init();

	while(1)
	{

		sensor_value = adc_read();
		printf("ADC conversion value: %d \n\r", sensor_value);
	}

}
