#include<uart.h>
#include<adc.h>
#include<systick.h>


#define RCC_GPIOD_EN			(1U<<3)
#define LED_PIN					12


int main()
{
	uart_TX_init();
	//Enable clock for GPIOD
	RCC_AHB1ENR |= (RCC_GPIOD_EN);

		//Set PD12 as output pin
	GPIOD_MODER |= (1U<<24);
	GPIOD_MODER &= ~(1U<25);

	systickDelayMs(0.001)

	while(1)
	{
		GPIOD->ODR |= LED_PIN;
		printf("")
	}
	return 0;
}

