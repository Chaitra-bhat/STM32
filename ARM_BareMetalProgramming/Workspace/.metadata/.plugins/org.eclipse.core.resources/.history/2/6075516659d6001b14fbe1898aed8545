#include <stm32f407xx.h>
//#include<stm32f4xx.h>


#define GPIOD_EN	(1U<<3)
#define PIN13		(1U<<13)
#define LED_PIN		PIN13

int main()
{
	//Enable clock
	RCC->AHB1ENR |= GPIOD_EN;
	GPIOD->MODER |= (1U<<26);
	GPIOD->MODER &= ~(1U<<27);

	while(1)
	{
		GPIOD->ODR ^= LED_PIN;
		for(int i; i<1000000;i++);
	}

	return 0;
}
