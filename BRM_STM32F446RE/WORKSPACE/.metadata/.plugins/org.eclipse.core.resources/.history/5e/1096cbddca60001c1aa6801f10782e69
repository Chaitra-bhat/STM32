#ifndef UART_H_
#define UART_H_
#include <stm32f4xx.h>
#include <stdint.h>
#include <stdio.h>

#define GPIOA_ENA		(1U<<0)
#define USART2ENA		(1U<<17)

#define SYS_FREQ		16000000
#define USARt_BAUDRATE  115200



#endif /* UART_H_ */
